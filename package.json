{
  "name": "amethyst",
  "productName": "Amethyst",
  "version": "0.2.0",
  "main": "./release/dist/main/main.js",
  "description": "A music box made of amethyst.",
  "scripts": {
    "dev": "concurrently \"npm run start:main\" \"npm run start:renderer\"",
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "tsc -p ./src/main/tsconfig.json",
    "build:renderer": "vite build --config ./src/renderer/vite.config.js",
    "start:main": "npm run build:main && cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only ./src/main/main.ts",
    "start:renderer": "vite dev --config ./src/renderer/vite.config.js",
    "package": "npm run build && electron-builder build --config .electron-builder.config.js --dir"
  },
  "old-scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.vue,.ts",
    "package": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build --publish never",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts && opencollective-postinstall",
    "dev": "ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only ./src/main/main.ts",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "jest",
    "prepare": "husky install"
  },
  "xornet-scripts": {
    "postinstall": "npm i -g nodemon typescript ts-node",
    "build": "node scripts/build.js",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "pretest": "npm run build",
    "test": "npx vitest --config vitest.config.ts",
    "dev": "npm run dev-staged",
    "dev-staged": "cross-env MODE=development-staged node scripts/watch.js",
    "dev-local": "cross-env MODE=development-local node scripts/watch.js",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "typecheck-electron": "tsc --noEmit -p packages/electron/tsconfig.json",
    "typecheck-preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck-vue": "vue-tsc --noEmit -p packages/vue/tsconfig.json",
    "typecheck": "npm run typecheck-electron && npm run typecheck-preload && npm run typecheck-vue",
    "pretypecheck-vue": "dts-cb -i packages/preload/src/**/*.ts -o packages/preload/exposedInMainWorld.d.ts",
    "prettify": "eslint --fix \"**/*.{vue,ts,js}\""
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "build": {
    "productName": "Amethyst",
    "appId": "org.erb.Amethyst",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "fileAssociations": [
      {
        "ext": "mp3"
      },
      {
        "ext": "wav"
      },
      {
        "ext": "flac"
      }
    ],
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "afterSign": ".erb/scripts/notarize.js",
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "amethyst",
      "repo": "amethyst"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Geoxor/amethyst"
  },
  "contributors": [
    {
      "name": "Connor OKeefe",
      "email": "cimok2000@gmail.com",
      "url": "https://github.com/cimok2000"
    },
    {
      "name": "Geoxor",
      "email": "geoxor123@outlook.com",
      "url": "https://github.com/Geoxor"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amethyst/amethyst/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "typescript",
    "ts",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "homepage": "https://github.com/amethyst/amethyst#readme",
  "jest": {
    "testURL": "http://localhost/",
    "testEnvironment": "jsdom",
    "transform": {
      "\\.(ts|vue|js|jsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules"
    ],
    "testPathIgnorePatterns": [
      "release/app/dist"
    ],
    "setupFiles": [
      "./.erb/scripts/check-build-exists.ts"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@types/howler": "^2.2.7",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.23",
    "@types/source-map-support": "^0.5.4",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "autoprefixer": "^10.4.7",
    "chalk": "^4.1.2",
    "concurrently": "^7.1.0",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "detect-port": "^1.3.0",
    "electron": "^18.0.1",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "^1.2.1",
    "electron-rebuild": "^3.2.7",
    "electronmon": "^2.0.2",
    "eslint": "^8.12.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.6.0",
    "opencollective-postinstall": "^2.0.3",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "url-loader": "^4.1.1",
    "vue-tsc": "^0.34.15"
  },
  "dependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vueuse/core": "^8.5.0",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.6",
    "electron-updater": "^4.6.5",
    "history": "^5.3.0",
    "howler": "^2.2.3",
    "music-metadata": "^7.12.3",
    "vite": "^2.9.9",
    "vite-plugin-windicss": "^1.8.4",
    "vue": "^3.2.33",
    "vue-router": "^4.0.13",
    "windicss": "^3.5.3"
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "browserslist": [],
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true
  },
  "electronmon": {
    "patterns": [
      "!src/__tests__/**",
      "!release/**"
    ],
    "logLevel": "quiet"
  }
}
