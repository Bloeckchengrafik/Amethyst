{
  "css.customData": [".vscode/css.json"],
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": false,
  },
  "typescript.inlayHints.variableTypes.enabled": false,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "search.exclude": {
    ".git": true,
    ".eslintcache": true,
    "release/{build,app/dist}": true,
    "node_modules": true,
    "npm-debug.log.*": true,
    "test/**/__snapshots__": true,
    "package-lock.json": true,
    "*.{css,sass,scss}.d.ts": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "assets/assets.d.ts": true,
    "assets/entitlements.mac.plist": true,
    "node_modules": true,
    "assets/icon.icns": true,
    ".husky": true,
    ".gitignore": true,
    ".eslintignore": true,
    ".gitattributes": true,
    "package-lock.json": true
  },
  "hide-files.files": [
    "assets/assets.d.ts",
    "assets/entitlements.mac.plist",
    "node_modules",
    "assets/icon.icns",
    ".husky",
    ".gitignore",
    ".eslintignore",
    ".gitattributes",
    "package-lock.json"
  ]
}
