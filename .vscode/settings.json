{
  "files.associations": {
    ".eslintrc": "jsonc",
    ".prettierrc": "jsonc",
    ".eslintignore": "ignore"
  },
  "css.customData": [".vscode/css.json"],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "html",
    "typescriptreact"
  ],
  "javascript.validate.enable": false,
  "javascript.format.enable": false,
  "typescript.format.enable": false,
  "search.exclude": {
    ".git": true,
    ".eslintcache": true,
    ".erb/dll": true,
    "release/{build,app/dist}": true,
    "node_modules": true,
    "npm-debug.log.*": true,
    "test/**/__snapshots__": true,
    "package-lock.json": true,
    "*.{css,sass,scss}.d.ts": true
  },
  "hide-files.files": [
    ".github",
    ".husky",
    "node_modules",
    "src/test",
    ".editorconfig",
    ".eslintcache",
    ".eslintignore",
    ".gitattributes",
    ".gitignore",
    "tsconfig.json",
    "assets/entitlements.mac.plist",
    "assets/assets.d.ts",
    "tailwind.config.js",
    "postcss.config.js",
    ".erb",
    "assets/icon.icns",
    ".vscode",
    ".eslintrc.js",
    "release/app",
    "release",
    "src/renderer/preload.d.ts",
    "package-lock.json",
    "package.json",
    "src/renderer/index.ejs",
    "assets/icons"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    ".github": true,
    ".husky": true,
    "node_modules": true,
    "src/test": true,
    ".editorconfig": true,
    ".eslintcache": true,
    ".eslintignore": true,
    ".gitattributes": true,
    ".gitignore": true,
    "tsconfig.json": true,
    "assets/entitlements.mac.plist": true,
    "assets/assets.d.ts": true,
    "tailwind.config.js": true,
    "postcss.config.js": true,
    ".erb": true,
    "assets/icon.icns": true,
    ".vscode": true,
    ".eslintrc.js": true,
    "release/app": true,
    "release": true,
    "src/renderer/preload.d.ts": true,
    "package-lock.json": true,
    "package.json": true,
    "src/renderer/index.ejs": true,
    "assets/icons": true
  }
}